{   
    "presets": [
		[
			"@babel/preset-env", {
				"targets": {
                    "ie": "10"
                },
                "corejs": 3,
				"useBuiltIns": "usage"
			}
		]
	],
	"plugins": [["@babel/plugin-transform-runtime", {
		"corejs": {
            "version": 3,
            "proposals": true
        },
        "helpers": true,
        "regenerator": true,
        "useESModules": true
	}]]
}